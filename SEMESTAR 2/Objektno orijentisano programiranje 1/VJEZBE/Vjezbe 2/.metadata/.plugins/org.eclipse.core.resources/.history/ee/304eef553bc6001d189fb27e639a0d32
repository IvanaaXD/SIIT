package vjezbe_2;

public class primjer6 {

	public static void main(String[] args) {
		
		int nizA[];
		
		nizA = new int [5];
		
		nizA[0] = 1;
		nizA[1] = 2;
		nizA[2] = 3;
		nizA[3] = 4;
		nizA[4] = 5;

		int nizB[] = {1, 2, 3, 4, 5};
		
		int broj = 2;
		int nizCijelihBrojeva[] = {7, 5, broj, 11, 3, -13, 1};
		
		double nizRealnihBrojeva[] = {Math.PI, 3.0, 2+3/5, Math.E, Math.sqrt(3)};
		
		String nizImena[] = {"Pera", "Mika", "Lazo"};

		System.out.println("Peti element niza A je: " + nizA[4]);
		
		System.out.println("Niz A je " + nizA.toString());
		
		for (int i = 0; i < nizA.length; i++) {
			System.out.println(i + "clan niza je " + nizA[i]);
		}
		
		int suma = 0;
		for (int i = 0; i < nizA.length; i++) {
			suma += i;
		}
		
		System.out.println("Suma niza A je: " + suma);
	}

}
