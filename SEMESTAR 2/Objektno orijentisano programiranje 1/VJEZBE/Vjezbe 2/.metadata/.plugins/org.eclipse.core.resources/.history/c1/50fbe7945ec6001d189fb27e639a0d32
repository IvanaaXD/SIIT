package vjezbe_2;

public class zadatak13 {

	public static void main(String[] args) {
		
		int matrica[][] = new int[5][6];
		
		int broj = 1;
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 6; j++) {
				matrica[i][j] = broj*broj;
			}
			broj += 1;
		}
		
		for (int i = 0; i < 5; i++) {
			System.out.println("\t");
			for (int j = 0; j < 6; j++) {
				System.out.print(matrica[i][j] + "   ");
			}
			System.out.print("\n");
		}
		
		int proizvod = 1;
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 6; j++) {
				
				if (i < j) {
					proizvod *= matrica[i][j];
				}
			}
		}
		
		System.out.println("Proizvod je: " + proizvod);
		
	}

}
