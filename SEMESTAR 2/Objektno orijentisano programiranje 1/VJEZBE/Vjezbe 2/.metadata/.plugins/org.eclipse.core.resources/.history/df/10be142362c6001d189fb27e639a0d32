package vjezbe_2;

public class zadatak15 {

	public static void main(String[] args) {

		int n = 7;
		String minus = "-";
		String zvijezda = "*";
		String wuhu = "";
		
		for (int i = 0; i < n*2 + 1; i++) {
			wuhu = minus.repeat(n) + zvijezda + minus.repeat(n);
			System.out.println(wuhu);
		}
		
	}

}
